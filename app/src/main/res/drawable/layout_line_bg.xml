<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">
    <item>
        <shape android:shape="rectangle">
            <solid android:color="#FFFFFF" />
        </shape>
    </item>
    <item
        android:left="-10dp"
        android:right="-10dp"
        android:top="-1dp">
        <shape >
            <stroke
                android:width="0.5dp"
                android:color="#dfdfdf"
                />
        </shape>
    </item>
</layer-list>